<% include ./partials/header %>
<H1> Index page!!</H1>


<%  blogs.forEach(function(blog){ %>
<div>
    <h2><%= blog.title %></h2>
    <h3><%= blog.image %></h3>
    <h5><%= blog.created %></h5>
    <p><%= blog.body.substring(0, 80) %></p>
    <a href="/blogs/<%= blog._id%>">Read More</a>

</div>

<% }); %>

